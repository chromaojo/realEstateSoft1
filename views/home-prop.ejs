<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>Property Biz - PB</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="/asset1/img/favicon.jpg" rel="icon">
  <link href="/asset1/img/apple-touch-icon.jpg" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link
    href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
    rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/asset1/vendor/aos/aos.css" rel="stylesheet">
  <link href="/asset1/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/asset1/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/asset1/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/asset1/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="/asset1/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/asset1/css/style.css" rel="stylesheet">

</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top">
    <div class="container-fluid d-flex justify-content-between align-items-center">

      <!-- <h1 class="logo me-auto me-lg-0"><a href="index.html">KSEPD</a></h1> -->
      <!-- Uncomment below if you prefer to use an image logo -->
      <a href="index.html" class="logo"><img src="/asset1/img/logo.png" alt="logo" class="img-fluid">
        <span>PROPERTY BIZ</span>
      </a>

      <nav id="navbar" class="navbar order-last order-lg-0">
        <ul>
          <li><a href="/">Home</a></li>
          <li><a  href="/about">About</a></li> 
          <li><a href="/pricing">Pricing</a></li>
          <li><a class="active" href="/properties">Properties</a></li>
        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

      <div class="header-social-links">
        <a href="#" class="twitter"><i class="bi bi-twitter"></i></a>
        <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
        <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
        <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></i></a>
      </div>

    </div>
  </header><!-- End Header -->

  <main id="main">

    <!-- ======= About Section ======= -->
    <section id="about" class="about">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>Our Properties</h2>
          <p>At Royal Reality Technology, we understand the challenges faced by real estate professionals. <br>That's
            why we created Property Tech, a cutting-edge software that simplifies property management, marketing, and
            sales.
            ...</p>
        </div>
        <div class="row">

          <!-- Sales Card -->

          <% if (userProp=='' ) { %>
            <br> <br>
            <h3 class="m-5 text-center mb-5">There is no property in the Archive</h3>

            <% } else{%>
              <% userProp.forEach(trans=> { %>

                <div class="col-xxl-6 col-xl-12">

                  <a href="<%= trans.pro_link %>" class="text-dark">
                    <div class="card">
                      <div class="row">
                        <div class="col-7 pt-2">
                          <div class="card-body">

                            <h5 class="card-title">
                              <%= trans.title.substring(0, 15)+( trans.title.length>15?'...': "") %>
                            </h5>
                            <small>Price: <%= trans.price %> NGN</small> <br>
                            <small class="card-text">
                              Location: <%= trans.location %>
                            </small>
                            <small class="mt-2">
                              <a href="/user/delet/<%= trans.prop_id %>" class="badge bg-danger">Remove From
                                Archive</a>
                            </small>
                          </div>
                        </div>
                        <div class="col text-center">
                          <img src="<%= trans.picture ? '/prop/'+trans.picture : 'assets/img/card.jpg' %>" width="100%"
                            class="p-3 pt-4" alt="property picture">
                        </div>
                      </div>

                    </div>
                  </a>

                </div><!-- End Customers Card -->

                <% }) %>
                  <% } %>
        </div>
      </div>
      </div>
      </div><!-- End Left side columns -->
      </div>

      </div>
    </section><!-- End About Section -->




  </main><!-- End #main -->


  <!-- ======= Footer ======= -->
  <footer id="footer">
    <div class="container">
      <div class="copyright">
        &copy; Copyright <strong><span>Property Biz Real Estate Management App</span></strong>. All Rights Reserved
      </div>
      <div class="credits">

        Devevloped by <a href="#">Royal Reality Technology</a>
      </div>
    </div>
  </footer><!-- End  Footer -->

  <div id="preloader"></div>
  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i
      class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="/asset1/vendor/purecounter/purecounter_vanilla.js"></script>
  <script src="/asset1/vendor/aos/aos.js"></script>
  <script src="/asset1/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/asset1/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="/asset1/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="/asset1/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="/asset1/vendor/waypoints/noframework.waypoints.js"></script>

  <!-- Template Main JS File -->
  <script src="/asset1/js/main.js"></script>

</body>

</html>